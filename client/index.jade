doctype html
html(ng-app="columbiaWync")
    head
        base(href='/')
        title columbiaWync
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width')
        link(rel='icon', href='assets/favicon.ico', type='image/x-icon')
        // build:css app.css
        // bower:css
        // endinject

        link(rel='stylesheet', href='../bower_components/bootstrap/dist/css/bootstrap.css')
        link(rel='stylesheet', href='bower_components/leaflet/dist/leaflet.css')
        link(rel='stylesheet', href='styles/css/app.css')
        // endbuild
    body
        .container
            .navbar.navbar-inverse.navbar-fixed-top
              .container
                  .navbar-header
                    button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                      span.icon-bar
                      |             
                      span.icon-bar
                      |             
                      span.icon-bar
                    |           
                    a.navbar-brand(href='index.html')
                      img.img.hidden-xs(src='assets/logo_footer.png', )
                      img.img.visible-xs(src='assets/logo_footer.png') 
                    
                    
                  .navbar-collapse.collapse
                    ul.nav.navbar-nav.navbar-right
                      li(class=(active === 'home' ? 'active' : ''))
                        a(href='index.html') Help         
                      li(class=(active === 'work' ? 'active' : ''))
                        a(href='work.html') Project information 
     
        div(ng-view='')

        .footer-v1
            .footer
                .container
                  .row
                    // About
                    .col-sm-3.md-margin-bottom-40
                      a(href='index.html')
                        img#logo-footer.footer-logo(src='http://lorempixel.com/g/100/100/transport')
                      |                         
                      p
                         | This is a GPS Mapping utility for Columbia University
                    .col-sm-3.md-margin-bottom-40
                    .col-sm-3.md-margin-bottom-40
                      .headline
                        h2 Useful Links
                      |                         
                      ul.list-unstyled.link-list
                        li
                          a(href='#') Columbia University
                          i.fa.fa-angle-right
                        |                             
                        li
                          a(href='#') Lamont
                          i.fa.fa-angle-right
                        |                             
                        li
                          a(href='#') Blah blah
                          i.fa.fa-angle-right
                    .col-sm-3.map-img.md-margin-bottom-40
                      .headline
                        h2 Contact Us
                      |                          
                      |                         
                      address.md-margin-bottom-40
                        | Darby Jack
                        br
                        |                             New York, NY 
                        br
                        |                             Phone: xx 

                        br
                        |                             Email: 
                        a(href='xxxx') xxxx@xxxx.com
                        br

                        a.tooltips(href='https://twitter.com/zevross', data-toggle='tooltip', data-placement='top', title='', data-original-title='Twitter')
                          i.fa.fa-twitter.twittercenter

        script(src='http://maps.google.com/maps/api/js?v=3&sensor=false')                  
        
        // build:js app.js
        // bower:js 
        script(src='bower_components/angular/angular.js')
        script(src='bower_components/angular-route/angular-route.js')
        script(src='bower_components/angular-resource/angular-resource.js')
        script(src='bower_components/angular-sanitize/angular-sanitize.js')
        script(src='bower_components/angular-animate/angular-animate.js')
        script(src='bower_components/angular-messages/angular-messages.js')
        script(src='bower_components/ng-file-upload/ng-file-upload-all.js')
        script(src='bower_components/leaflet/dist/leaflet.js')
        script(src='bower_components/angular-leaflet-directive/dist/angular-leaflet-directive.js')
        script(src='bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js')
        script(src='bower_components/jquery/dist/jquery.js')
        script(src='bower_components/bootstrap/dist/js/bootstrap.js')
        // endinject 
        
        script(src='http://tombatossals.github.io/angular-leaflet-directive/bower_components/leaflet-plugins/layer/tile/Google.js')
      
        // inject:js  
        script(src='modules/static-json-module.js') 
        script(src='app.js')
        script(src='views/home/home.js')
        script(src='views/home/home.controller.js')
        script(src='services/leaflet-route.srvc.js')
        // endinject
        // endbuild
